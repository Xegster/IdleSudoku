{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "expo:start",
      "type": "shell",
      "command": "npm",
      "args": ["start"],
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": "^$",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": ".*Metro waiting on.*"
        }
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    },
    {
      "label": "expo:start-web",
      "type": "shell",
      "command": "npm",
      "args": ["run", "web"],
      "isBackground": true,
      "options": {
        "env": {
          "EXPO_DOCTOR_SKIP_DEPENDENCY_VERSION_CHECK": "1"
        }
      },
      "problemMatcher": {
        "pattern": {
          "regexp": "^$",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": ".*(webpack compiled|Web is running|Metro waiting|ready - started server).*"
        }
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    },
    {
      "label": "expo:start-android",
      "type": "shell",
      "command": "npm",
      "args": ["run", "android"],
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": "^$",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": ".*Metro waiting on.*"
        }
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    }
  ]
}
